{
  "name": "php-acl-access",
  "scripts": {
    "test": "eslint resources/javascript/ --ext .ts",
    "fix": "eslint resources/javascript/ --ext .ts --fix",
    "watch:admin": "parcel watch --out-dir=public/admin/ resources/javascript/admin/*.ts --no-source-maps --no-hmr",
    "build:admin": "parcel build --out-dir=public/admin/ resources/javascript/admin/*.ts --no-source-maps",
    "watch:global": "parcel watch --out-dir=public/ resources/javascript/*.ts --no-source-maps --no-hmr",
    "build:global": "parcel build --out-dir=public/ resources/javascript/*.ts --no-source-maps"
  },
  "dependencies": {
    "bootstrap": "^4.5.3",
    "jquery": "^3.5.1",
    "popper.js": "^1.16.1"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/plugin-transform-runtime": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@babel/runtime": "^7.12.5",
    "@typescript-eslint/eslint-plugin": "^4.10.0",
    "@typescript-eslint/parser": "^4.10.0",
    "eslint": "^7.16.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.1.0",
    "npm-run-all": "^4.1.5",
    "parcel-bundler": "^1.12.4",
    "sass": "^1.30.0",
    "typescript": "^4.1.3"
  }
}
