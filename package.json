  "scripts": {
    "lint": "eslint resources/javascript/ --ext .ts",
    "lint:fix": "eslint resources/javascript/ --ext .ts --fix",
    "watch:admin": "parcel watch --out-dir=public/admin/ resources/javascript/admin/*.ts --no-source-maps --no-hmr",
    "build:admin": "parcel build --out-dir=public/admin/ resources/javascript/admin/*.ts --no-source-maps",
    "watch:global": "parcel watch --out-dir=public/ resources/javascript/*.ts --no-source-maps --no-hmr",
    "build:global": "parcel build --out-dir=public/ resources/javascript/*.ts --no-source-maps"
  },
  "dependencies": {
    "bootstrap": "^4.6.0",
    "jquery": "^3.6.0",
    "popper.js": "^1.16.1"
  },
  "devDependencies": {
    "@babel/core": "^7.13.15",
    "@babel/plugin-transform-runtime": "^7.13.15",
    "@babel/preset-env": "^7.13.15",
    "@babel/runtime": "^7.13.10",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/parser": "^4.22.0",
    "eslint": "^7.24.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.1.0",
    "eslint-plugin-standard": "^5.0.0",
    "npm-run-all": "^4.1.5",
    "parcel-bundler": "^1.12.4",
    "sass": "^1.32.8",
    "typescript": "^4.2.4"
  }
